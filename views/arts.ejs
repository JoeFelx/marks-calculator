                    <input type="text" name="twelthAccontancy" autocomplete="off">
                </div>
                <div class="economics-12th">
                    <label>Economics Assessment 10 Marks</label><br>
                    <input type="text" name="twelthEconomics" autocomplete="off">
                </div>
                <div class="ca-12th">
                    <label>Computer Application Practical 30 Marks</label><br>
                    <input type="text" name="twelthCA" autocomplete="off">
                </div>
