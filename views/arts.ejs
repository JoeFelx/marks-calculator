                    <input type="text" name="eleventhCA" autocomplete="off" placeholder="100">
                </div>
                <div class="accontancy-11th">
                    <label>Accontancy</label><br>
                    <input type="text" name="eleventhAccontancy" autocomplete="off" placeholder="100">
                </div>
                <div class="economics-11th">
                    <label>Economics</label><br>
                    <input type="text" name="eleventhEconomics" autocomplete="off" placeholder="100">
                </div>
            </div>
            <div class="twelth">
